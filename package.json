{
  "name": "jasminetea",
  "version": "0.2.0-beta.1",
  "description": "Quick start Jasmine2 for CoffeeScript",

  "bin" : "jasminetea",
  "files": [
    "jasminetea",
    "src"
  ],

  "config": {
    "ghooks": {
      "pre-commit": "npm run cover"
    }
  },
  "scripts": {
    "default": "jasminetea test --verbose --timeout 1000",
    "cover": "$(opc package scripts.default) --cover --report",

    "Developer commands": "",
      "start": "$(opc package scripts.default) --watch --stacktrace",

    "Travis-CI commands": "",
      "test": "npm run cover",

    "Npm update via Travis-CI": "",
      "prepublish": "node -e if(!process.env.TRAVIS)process.exit(1); && npm run compile",
      
      "compile": "coffee --bare --compile index.coffee",
      "postcompile": "coffee --output lib --bare --compile src"
  },

  "dependencies": {
    "bluebird": "^2.9.25",
    "chalk": "^1.0.0",
    "glob-watcher": "^2.0.0",
    "coffee-script": "^1.9.2",
    "coffeelint": "^1.9.2",
    "commander": "^2.6.0",
    "coveralls": "^2.11.2",
    "ibrik": "^2.0.0",
    "jasmine": "^2.2.1",
    "jasmine-terminal-reporter": "^0.9.1",
    "rimraf": "^2.2.8",
    "wanderer": "^0.0.3-alpha.0"
  },
  "devDependencies": {
    "ghooks": "^0.3.2",
    "object-parser-cli": "0.0.0-alpha.0"
  },
  "globalDependencies": {
    "jasminetea": "0.1.30-rc.3"
  },
  
  "keywords": [
    "coffee-script",
    "jasmine2",
    "coverage",
    "lint",
    "watch",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/jasminetea.git"
  },
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/59naga/jasminetea/issues"
  },
  "homepage": "https://github.com/59naga/jasminetea"
}
