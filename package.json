{
  "name": "jasminetea",
  "bin":  "jasminetea",
  "main": "jasminetea.pomace.js",
  "version": "0.1.9",

  "description": "Quick-start jasmine-CLI for CoffeeScript",
  "scripts": {
    "start": "node jasminetea . -vl -t 10000 -w",

    "help":  "node jasminetea",
    "watch": "node jasminetea test -rw",
    "lint":  "node jasminetea . -l -t 10000",
    "cover": "node jasminetea . -c -t 10000",
    
    "test":  "node jasminetea . -vct 10000",
    "coveralls": "coveralls < ./coverage/lcov.info && rm -rf coverage"
  },

  "dependencies": {
    "chalk": "^1.0.0",
    "coffee-script": "^1.9.0",
    "coffeelint": "^1.9.2",
    "commander": "^2.6.0",
    "gulp": "^3.8.11",
    "gulp-jasmine": "^2.0.0",
    "gulp-src-files": "0.0.16",
    "gulp-watch": "^4.1.1",
    "ibrik": "^2.0.0",
    "istanbul": "^0.3.6",
    "node-module-all": "0.0.6",
    "rimraf": "^2.2.8"
  },
  "devDependencies": {
    "coveralls": "^2.11.2"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/jasminetea.git"
  },
  "keywords": [
    "jasmine",
    "coverage",
    "lint",
    "coffee-script",
    "cli"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/59naga/jasminetea/issues"
  },
  "homepage": "https://github.com/59naga/jasminetea"
}
