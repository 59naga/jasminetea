{
  "name": "jasminetea",
  "bin":  "jasminetea",
  "main": "jasminetea.pomace.js",
  "version": "0.1.4",
  "description": "Quick-start jasmine-CLI for CoffeeScript",

  "scripts": {
    "pretest": "npm uninstall jasminetea -g",
    "test": "./jasminetea . -vt 10000 && npm link && NODE_PATH=`npm root -g` jasminetea . -vt 10000",
    "posttest": "npm uninstall jasminetea -g",

    "coverage": "ibrik cover onefile.coffee && istanbul report html",
    "coveralls": "coveralls < ./coverage/lcov.info && rm -rf coverage",

    "help": "node jasminetea",
    "start": "node jasminetea . -vw \"*.coffee,lib/**/*.coffee\" -t 10000 -l \"*.coffee\""
  },

  "dependencies": {
    "chalk": "^1.0.0",
    "coffee-script": "^1.9.0",
    "coffeelint": "^1.9.2",
    "commander": "^2.6.0",
    "gulp": "^3.8.11",
    "gulp-jasmine": "^2.0.0",
    "gulp-src-files": "0.0.15",
    "gulp-watch": "^4.1.1",
    "node-module-all": "^0.0.5"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.6",
    "ibrik": "^2.0.0"
  },
  
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/jasminetea.git"
  },
  "keywords": [
    "jasmine",
    "gulp-jasmine",
    "cli"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/59naga/jasminetea/issues"
  },
  "homepage": "https://github.com/59naga/jasminetea"
}
